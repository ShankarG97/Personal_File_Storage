<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.infovenz.sportz">

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_img"
        android:label="@string/app_name"
        android:roundIcon="@drawable/logo_img"
        android:supportsRtl="true"
        android:theme="@style/Theme.Sportz">
        <activity android:name=".AddAddress"></activity>
        <activity android:name=".Payment_Success" />
        <activity android:name=".PaymentActivity" />
        <activity android:name=".MycartPage" />
        <activity android:name=".ProductDetails" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".IntroActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>